{% extends 'Admin/base.html.twig' %}

{% block title %} Albums form{% endblock %}

{% block body %}

<section class="container" id="section-home">
    <div class="col-md-9 card" style="min-height:25em; color: white; padding: 2em;">
        {{ form_start(form) }}
            {{ form_row(form.name) }}
            {{ form_row(form.date) }}
            {{ form_row(form.imageFile) }}
            {{ form_row(form.price) }}
            {{ form_row(form.types) }}
            {{ form_row(form.artists) }}
            {{ form_row(form.submit) }}
        {{ form_end(form) }}    
    </div>
</section>    

{% endblock %}